function _note() {
  COMPREPLY=()
  local cur="${COMP_WORDS[COMP_CWORD]}"
  local prev="${COMP_WORDS[COMP_CWORD-1]}"

  case "${prev}" in
    note)
      COMPREPLY=( $(compgen -W "pull push publish delete" -- ${cur}) )
      ;;
    pull)
      COMPREPLY=( $(compgen -W "`ls`" -- ${cur}) )
      ;;
    push)
      ;;
    publish)
      ;;
    delete)
      ;;
  esac

}

complete -F _note note

